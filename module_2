    "## Анализ номинативных переменных"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 89,
   "id": "427c5a6f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Построим боксплоты для нечисловых данных и целевого параметра \"score\" (количество баллов за экзамен по математике)\n",
    "def boxplot_sm(column):\n",
    "    fig, ax = plt.subplots()\n",
    "    sns.boxplot(x=column, y='score', \n",
    "                data=sm.loc[sm.loc[:, column].isin(sm.loc[:, column].value_counts().index[:10])],\n",
    "               ax=ax)\n",
    "    plt.xticks(rotation=45)\n",
    "    ax.set_title('Boxplot for ' + column)\n",
    "    plt.show()"
   ]
  },
