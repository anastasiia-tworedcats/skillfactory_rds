    "\n",
    "# Попробуем найти выбросы в значениях данного столбца.\n",
    "iqr = sm.absences.quantile(0.75) - sm.absences.quantile(0.25)\n",
    "perc25 = sm.absences.quantile(0.25)\n",
    "perc75 = sm.absences.quantile(0.75)\n",
    " \n",
    "print('25-й перцентиль:', perc25)\n",
    "print('75-й перцентиль:', perc75)\n",
    "print(\"IQR\", iqr)\n",
    "print(\"Границы выбросов:\", perc25 - 1.5*iqr, perc75 + 1.5*iqr)\n",
    "\n",
    "sm.absences = sm.absences.loc[sm.absences.between(\n",
    "perc25 - 1.5*iqr,\n",
    "perc75 + 1.5*iqr)]\n",
    "\n",
    "sns.distplot(sm['absences'])"
   ]
